    "massive": "^2.1.0",
    "material-ui": "^0.15.0",
    "pg": "^4.5.5",
    "pg-simple": "^0.2.2",
    "react": "^0.14.6",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.0",
